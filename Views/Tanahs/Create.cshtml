@model UCP1_PAW_097_C.Models.Tanah

@{
    ViewData["Title"] = "Create";
}

<h1>Tambah</h1>

<h4>Data Tanah</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="IdTanah" class="control-label">Id Tanah</label>
                <input asp-for="IdTanah" class="form-control" />
                <span asp-validation-for="IdTanah" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="JudulTanah" class="control-label">Judul Tanah</label>
                <input asp-for="JudulTanah" class="form-control" />
                <span asp-validation-for="JudulTanah" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AlamatTanah" class="control-label"></label>
                <textarea asp-for="AlamatTanah" class="form-control" rows="3"></textarea>
                <span asp-validation-for="AlamatTanah" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Status" class="control-label">Status</label>
                <input asp-for="Status" class="form-control" />
                <span asp-validation-for="Status" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Harga" class="control-label">Harga</label>
                <input asp-for="Harga" class="form-control" />
                <span asp-validation-for="Harga" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Luas" class="control-label">Luas (Ha)</label>
                <input asp-for="Luas" class="form-control" />
                <span asp-validation-for="Luas" class="text-danger"></span>
            </div>


@*            <div class="form-group">
                <label asp-for="Foto" class="control-label"></label>
                <input type="file" name="Files" multiple />
                <span asp-validation-for="Foto" class="text-danger"></span>
            </div>*@


            <div class="form-group">
                <input type="submit" value="Tambah" class="btn btn-primary" />
                <a class="btn btn-outline-danger" asp-action="Index">Kembali</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
